{
  "name": "flashcards-server",
  "version": "0.1.0",
  "description": "Flashcards Server",
  "type": "module",
  "main": "./src/app.ts",
  "scripts": {
    "dev:local": "tsx --watch --env-file=.env ./src/server-with-local.ts",
    "dev:mysql": "tsx --watch --env-file=.env ./src/server-with-mysql.ts",
    "start": "jest && tsx --watch --env-file=.env ./src/server-with-local.ts",
    "test": "jest --watch",
    "prettier": "npx prettier . --write",
    "lint": "eslint --fix . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "npm run prettier && npm run lint"
  },
  "keywords": [],
  "author": "Aitor Melero",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.7.0",
    "mysql2": "3.9.7",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "supertest": "7.0.0",
    "ts-jest": "29.1.2",
    "tsx": "4.7.3",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.19.2",
    "zod": "3.23.8"
  }
}
